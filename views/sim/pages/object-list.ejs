 


<div class="card">
    <div class="card-header">
      Marker
    </div>
    <div class="card-body" style="overflow: auto !important;max-height: 75vh !important;">
     
  

            
        <form id="gen-marker">
       
            <div class="form-group row">
                <label for="objectId" class="col-sm-3 col-form-label">ID</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control form-control-sm" id="marker-id" name="marker-id" placeholder="Marker ID">
                </div>
            </div>
            <div class="form-group row">
                <label for="marker-type" class="col-sm-3 col-form-label">Type</label>
                <div class="col-sm-9">
                    <select class="form-control select2" id="marker-type" name="marker-type">
                        <option value="bar">Bar</option> 
                        <option value="diamond">Diamond</option>
                        <option value="cross">Cross</option> 
                        <option value="ellipse">Ellipse</option> 
                        <option value="pie">Pie</option> 
                        <option value="pin"  selected>Pin</option> 
                        <option value="rectangle">Rectangle</option> 
                        <option value="square">Square</option> 
                        <option value="triangle">Triangle</option> 
                        <option value="x">X</option> 
                     
                        
                     </select>           
                </div>
            </div>
            <div class="form-group row">
                <label for="marker-role" class="col-sm-3 col-form-label">Group</label>
                <div class="col-sm-9">
                    <select class="form-control select2" id="marker-role" name="marker-role"  >
                        <option value="" >Select Marker Role</option> 
                        <option value="Accident">Accident</option>
                        <option value="Barricade">Barricade</option> 
                        <option value="Crime Scene">Crime Scene</option>
                        <option value="Explosion">Explosion</option> 
                        <option value="Fire">Fire</option> 
                        <option value="Incident">Incident</option> 
                        <option value="Police Deployment">Police Deployment</option>
                        <option value="Protest">Protest</option> 
                        <option value="Public Disturbance ">Public Disturbance </option> 
                        <option value="Riot Deployment">Traffic Deployment</option>
                        <option value="Road Block">Road Block</option> 
                        <option value="Road Closed">Road Closed</option> 
                        <option value="Swat Deployment">Swat Deployment</option>
                        <option value="Traffic Deployment">Traffic Deployment</option>
                        <option value="Vehicle Check Point (VCP)">Vehicle Check Point (VCP)</option>  
                     </select>           
                </div>
            </div>



            <div class="form-group row">
                <label for="marker-title" class="col-sm-3 col-form-label">Title</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control form-control-sm" id="marker-title"name="marker-title" placeholder="Title">
                </div>
            </div>
            <div class="form-group row">
                <label for="marker-content" class="col-sm-3 col-form-label">Content</label>
                <div class="col-sm-9">
                    <textarea type="text"  rows="3" class="form-control form-control-sm" id="marker-content"  name="marker-content" placeholder="Content"></textarea>
                </div>
            </div>
            <div class="form-group row">
                <label for="marker-color" class="col-sm-3 col-form-label">Color</label>
                <div class="col-sm-9">
                    <!-- Easy and useful color picker -->
                    <input type="color" value="#000000" id="marker-color" name="marker-color"  class="inputColor">
                 </div>
            </div>
            <div class="form-group row">
                <label for="inputPassword" class="col-sm-3 col-form-label">Hight</label>
                <div class="col-sm-9">
                    <select class="form-control select2" id="marker-floor" name="marker-floor" >
                        <option value="0" >Select Floor</option> 
                        <option value="1">1</option> 
                        <option value="2">2</option> 
                        <option value="3">3</option> 
                        <option value="4">4</option> 
                        <option value="5">5</option> 
                        <option value="6">6</option> 
                        <option value="7">7</option> 
                        <option value="8">8</option> 
                        <option value="9">9</option> 
                        <option value="10">10</option> 
                        <option value="11">11</option> 
                        <option value="12">12</option> 
                        <option value="13">13</option> 
                        <option value="14">14</option> 
                        <option value="15">15</option> 
                        <option value="16">16</option> 
                        <option value="17">17</option> 
                        <option value="18">18</option> 
                        <option value="19">19</option> 
                        <option value="20">20</option> 
                        <option value="21">21</option> 
                        <option value="22">22</option> 
                        <option value="23">23</option> 
                        <option value="24">24</option> 
                        <option value="25">25</option> 
                     </select>                
                    </div>
                    
            </div>
            <div class="form-group row">
                <div class="col-sm-12">
                    <button type="button" onclick="rd.getFormValues('gen-marker');" class="btn btn-success">Add</button>
                </div>
            </div>
            </form>




</div>
</div>
